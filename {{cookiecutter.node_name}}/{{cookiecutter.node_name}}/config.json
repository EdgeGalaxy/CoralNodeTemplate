{
    "node_id": "{{ cookiecutter.node_name | lower }}",
    "meta": {
        {%- if cookiecutter.node_type|lower != 'dataproducernode' %}
        "receivers": [{ "node_id": "{{ cookiecutter.receiver_node }}" }],
        {%- endif %}
        {%- if cookiecutter.node_type|lower != 'endnode' %}
        "sender": { "node_id": "{{ cookiecutter.node_slug }}" }
        {%- endif %}
    },
    "generic": {
        "enable_metrics": true,
        "metrics_sender": {
            "node_id": "{{ cookiecutter.node_slug }}_metrics",
            "raw_type": "Metrics"
        }
    },
    "params": {}
}